{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never"
    },
    "no-shadowed-variable": false,
    "curly": false,
    "object-literal-sort-keys": false,
    "jsx-boolean-value": {
      "options": "never"
    },
    "jsx-curly-spacing": {
      "options": "never"
    },
    "jsx-equals-spacing": {
      "options": "never"
    },
    "jsx-key": true,
    "jsx-self-close": true,
    "jsx-space-before-trailing-slash": true,
    "jsx-wrap-multiline": true,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-alignment": true
  },
  "rulesDirectory": []
}
